<div id="main_content">
  <div id="header">
    <p>Library System</p>
    <div class="buttons">
      <button class="button"><%= link_to "Logout", :controller => 'Access', :action => 'login' %></button>
    </div>
  </div>
</div>
<div id="sidebar">
<div id="logo">
<%= link_to root_path do %>
<!--<%= image_tag "logo.jpg" %>-->
<% end %>
</div>
<ul>
	<li class="category"><%= link_to "Profile", :action => 'userprofilebyuser' %></li>
	<li class="category">View History</li>
		<li class="category"><%= link_to "View/Book Room", :controller => 'access',:action => 'viewroomdetailsbyuser' %></li>

</ul>
</div>
<body>
	<table style="width:100%">
		<tr>
			<th> Room number</th>
			<th>Booking Date</th>
			<th>Start Time</th>
		</tr>

		<% @room.each do |roomadd| %>
		<tr>
			<td><%= roomadd.roomnum %></td>
			<td><%= roomadd.booking_date %></td>
			<td><%= roomadd.start_time.hour %></td>
			<td><%= link_to 'Delete', :controller => 'Access' , :action => 'dest' , :roomnum => roomadd.id,method: :delete %></td>

				</tr>
		<% end %>
	</table>

</body>